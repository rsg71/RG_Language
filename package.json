{
  "name": "rglanguage",
  "version": "1.0.0",
  "description": "RG Language",
  "main": "index.ts",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "npm install && node dist/index.js",
    "start:dev": "set NODE_ENV=dev&& concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "postinstall": "tsc",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npx tsc && npm run build",
    "ts-build": "npx tsc",
    "start-app-typescript:dev": "set NODE_ENV=dev&& node dist/index.js | pino-pretty",
    "start-app-typescript:prod": "node dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/node": "^18.6.1",
    "@types/passport": "^1.0.9",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "pino": "^8.4.1",
    "pino-pretty": "^9.0.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.16.3",
    "express-session": "^1.17.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.15",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "react": "^18.2.0"
  }
}
